{
  "name": "ngrok Client",
  "version": "latest",
  "slug": "ngrok",
  "description": "A ngrok client for Hass.io",
  "webui": "http://[HOST]:[PORT:4040]",
  "url": "https://github.com/ThePicklenat0r/hassio-ngrok",
  "host_network": "true",
  "ingress": true,
  "ingress_port": 4040,
  "ingress_entry": "/inspect/http",
  "panel_icon": "mdi:wan",
  "panel_title": "ngrok Client",
  "panel_admin": true,
  "arch": [
    "amd64",
    "i386"
  ],
  "startup": "system",
  "boot": "auto",
  "ports": {
    "4040/tcp": null
  },
  "ports_description": {
    "4040/tcp": "Web interface (Not required for Hass.io Ingress)"
  },
  "options": {
    "NGROK_AUTH": "",
    "NGROK_SUBDOMAIN": "",
    "NGROK_HOSTNAME": "",
    "NGROK_INSPECT": false,
    "PORT_80": true,
    "PORT_443": true,
    "PORT_8123": false
  },
  "schema": {
    "NGROK_AUTH": "str?",
    "NGROK_SUBDOMAIN": "str?",
    "NGROK_HOSTNAME": "str?",
    "NGROK_REGION": "list(us|eu|ap|au|sa|jp|in)?",
    "NGROK_INSPECT": "bool",
    "PORT_80": "bool",
    "PORT_443": "bool",
    "PORT_8123": "bool"
  },
  "image": "thepicklenat0r/hassio-ngrok"
}